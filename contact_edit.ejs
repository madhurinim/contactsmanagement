<% include partials/header %>

<div class="container">
    <div class="row mt-5">
        <div class="col-md-5">
            <div class="card">
                <div class="card-body">
                    <form action="/update/<%= info.contactid %>" method="post">
                        <div class="form-group">
                            <input type="text" name="fname" id="" value="<%= info.fname %>" class="form-control">
                        </div>
                        <div class="form-group">
                            <input type="text" name="mname" id="" value="<%= info.mname %>" class="form-control">
                        </div>
                        <div class="form-group">
                            <input type="text" name="lname" id="" value="<%= info.lname %>" class="form-control">
                        </div>
                         <div class="form-group">
                            <input type="text" name="addresstype" id="" value="<%= info.addresstype %>" class="form-control">
                        </div>
                         <div class="form-group">
                            <input type="text" name="address" id="" value="<%= info.address %>" class="form-control">
                        </div>
                         <div class="form-group">
                            <input type="text" name="city" id="" value="<%= info.city %>" class="form-control">
                        </div>
                         <div class="form-group">
                            <input type="text" name="state" id="" value="<%= info.state %>" class="form-control">
                        </div>
                         <div class="form-group">
                            <input type="text" name="zip" id="" value="<%= info.zip %>" class="form-control">
                        </div>
                         <div class="form-group">
                            <input type="text" name="phonetype" id="" value="<%= info.phonetype %>" class="form-control">
                        </div>
                         <div class="form-group">
                            <input type="text" name="number" id="" value="<%= info.number %>" class="form-control">
                        </div>
                         <div class="form-group">
                            <input type="text" name="areacode" id="" value="<%= info.areacode %>" class="form-control">
                        </div>
                         <div class="form-group">
                            <input type="text" name="datetype" id="" value="<%= info.datetype %>" class="form-control">
                        </div>
                         <div class="form-group">
                            <input type="text" name="date" id="" value="<%= info.date %>" class="form-control">
                        </div>
                        <button type="submit" class="btn btn-primary">Click here to complete edit</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

<% include partials/footer %>

